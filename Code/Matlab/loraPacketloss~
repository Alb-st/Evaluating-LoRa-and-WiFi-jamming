clc
clear
format long
jammer = 0:0.1:2;

lora00 = readtable("loradata/lora00");
lora01 = readtable("loradata/lora01");
lora02 = readtable("loradata/lora02");
lora03 = readtable("loradata/lora03");
lora04 = readtable("loradata/lora04");
lora05 = readtable("loradata/lora05");
lora06 = readtable("loradata/lora06");
lora07 = readtable("loradata/lora07");
lora08 = readtable("loradata/lora08");
lora09 = readtable("loradata/lora09");
lora10 = readtable("loradata/lora10");
lora11 = readtable("loradata/lora11");
lora12 = readtable("loradata/lora12");
lora13 = readtable("loradata/lora13");
lora14 = readtable("loradata/lora14");
lora15 = readtable("loradata/lora15");
lora16 = readtable("loradata/lora16");
lora17 = readtable("loradata/lora17");
lora18 = readtable("loradata/lora18");
lora19 = readtable("loradata/lora19");
lora20 = readtable("loradata/lora20");

loraPacketloss = zeros(1,21);
loraMaxRtt = zeros(1,21);
loraMinRtt = zeros(1,21);

loraPacketloss(1) = size(table2array(lora00(:,15)),1);
loraPacketloss(2) = size(table2array(lora01(:,15)),1);
loraPacketloss(3) = size(table2array(lora02(:,15)),1);
loraPacketloss(4) = size(table2array(lora03(:,15)),1);
loraPacketloss(5) = size(table2array(lora04(:,15)),1);
loraPacketloss(6) = size(table2array(lora05(:,15)),1);
loraPacketloss(7) = size(table2array(lora06(:,15)),1);
loraPacketloss(8) = size(table2array(lora07(:,15)),1);
loraPacketloss(9) = size(table2array(lora08(:,15)),1);
loraPacketloss(10) = size(table2array(lora09(:,15)),1);
loraPacketloss(11) = size(table2array(lora10(:,15)),1);
loraPacketloss(12) = size(table2array(lora11(:,15)),1);
loraPacketloss(13) = size(table2array(lora12(:,15)),1);
loraPacketloss(14) = size(table2array(lora13(:,15)),1);
loraPacketloss(15) = size(table2array(lora14(:,15)),1);
loraPacketloss(16) = size(table2array(lora15(:,15)),1);
loraPacketloss(17) = size(table2array(lora16(:,15)),1);
loraPacketloss(18) = size(table2array(lora17(:,15)),1);
loraPacketloss(19) = size(table2array(lora18(:,15)),1);
loraPacketloss(20) = size(table2array(lora19(:,15)),1);
loraPacketloss(21) = size(table2array(lora20(:,15)),1);

for i=1:length(loraPacketloss)


end



plot(jammer, loraPacketloss, 'b')
hold on

title("LoRa RTT plotted against jamming noise")
ylabel("Packets lost ()")
xlabel("Noise")

hold off
shg

